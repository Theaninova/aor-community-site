{
  "name": "aor-mods",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "dev:external": "vite dev --host 0.0.0.0",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "optimize": "NODE_OPTIONS='--no-experimental-fetch' gulp",
    "unity-import": "node unity-parser/stats-importer.js",
    "unity-parser": "node --loader ts-node/esm --experimental-specifier-resolution=node unity-parser/parse-scene.ts",
    "import:cars": "ts-node-esm unity-parser/car-importer.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "lint:styles": "stylelint src/**/*.{css,scss,svelte} --fix",
    "format": "prettier --write --plugin-search-dir=. .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": "^16.18.0"
  },
  "stylelint": {
    "extends": "@theaninova/stylelint-config-standard"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@storybook/addon-actions": "7.0.23",
    "@storybook/addon-essentials": "7.0.23",
    "@storybook/addon-interactions": "7.0.23",
    "@storybook/addon-links": "7.0.23",
    "@storybook/sveltekit": "7.0.23",
    "@storybook/testing-library": "0.2.0",
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "1.20.5",
    "@theaninova/prettier-config": "1.0.0",
    "@theaninova/stylelint-config-standard": "1.0.1",
    "@threlte/core": "5.0.9",
    "@threlte/extras": "4.9.3",
    "@threlte/preprocess": "0.0.2",
    "@types/gulp": "4.0.11",
    "@types/gulp-rename": "2.0.2",
    "@types/js-yaml": "4.0.5",
    "@types/three": "0.149.0",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "babel-loader": "9.1.2",
    "dotenv": "16.3.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-storybook": "0.6.12",
    "eslint-plugin-svelte3": "4.0.0",
    "flag-icons": "6.7.0",
    "glob": "10.3.0",
    "gulp": "4.0.2",
    "gulp-libsquoosh": "1.1.2",
    "gulp-rename": "2.0.0",
    "image-js": "0.35.4",
    "js-yaml": "4.1.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.63.6",
    "storybook": "7.0.23",
    "stylelint": "15.9.0",
    "svelte": "4.0.0",
    "svelte-check": "3.4.4",
    "svelte-loader": "3.1.8",
    "svelte-preprocess": "5.0.4",
    "svelte-sequential-preprocessor": "1.0.0",
    "swiper": "9.4.1",
    "postcss-html": "1.5.0",
    "three": "0.149.0",
    "ts-node": "10.9.1",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "vite": "4.3.9"
  },
  "type": "module"
}
